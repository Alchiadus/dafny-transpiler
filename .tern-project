{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "lib/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**/*"
  ],
  "plugins": {
    "complete_strings": {},
    "doc_comment": {
      "fullDocs": true
    },
    "modules": {},
    "es_modules": {},
    "node": {},
    "node_resolve": {}
  }
}
